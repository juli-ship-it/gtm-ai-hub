# Supabase
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Authentication Settings
NEXT_PUBLIC_RESTRICT_TO_DOMAINS=true
NEXT_PUBLIC_ALLOWED_DOMAINS=yourcompany.com,partner.com

# n8n Integration
N8N_BASIC_USER=your_n8n_username
N8N_BASIC_PASS=your_n8n_password
N8N_WEBHOOK_SECRET=your_n8n_webhook_secret

# HubSpot
HUBSPOT_CLIENT_ID=your_hubspot_client_id
HUBSPOT_CLIENT_SECRET=your_hubspot_client_secret
HUBSPOT_REDIRECT_URI=http://localhost:3000/auth/hubspot/callback

# Google Analytics 4
GA4_SA_CLIENT_EMAIL=your_ga4_service_account_email
GA4_SA_PRIVATE_KEY=your_ga4_private_key

# Snowflake
SNOWFLAKE_ACCOUNT=your_snowflake_account
SNOWFLAKE_USER=your_snowflake_user
SNOWFLAKE_ROLE=your_snowflake_role
SNOWFLAKE_WAREHOUSE=your_snowflake_warehouse
SNOWFLAKE_DB=your_snowflake_database
SNOWFLAKE_PRIVATE_KEY=your_snowflake_private_key

# Snowflake MCP (Model Context Protocol) - Using Snowflake Managed MCP Server
SNOWFLAKE_MCP_ENABLED=true
SNOWFLAKE_MCP_READ_ONLY=true
SNOWFLAKE_MCP_MAX_ROWS=1000
SNOWFLAKE_MCP_TIMEOUT=300
SNOWFLAKE_MCP_AUDIT_LOGGING=true
SNOWFLAKE_MCP_OAUTH_CLIENT_ID=your_oauth_client_id
SNOWFLAKE_MCP_OAUTH_CLIENT_SECRET=your_oauth_client_secret
SNOWFLAKE_MCP_OAUTH_REDIRECT_URI=http://localhost:3000/auth/snowflake/callback

# Mixpanel
MIXPANEL_PROJECT_ID=your_mixpanel_project_id
MIXPANEL_SERVICE_ACCOUNT_SECRET=your_mixpanel_service_account_secret
MIXPANEL_API_SECRET=your_mixpanel_api_secret

# Mixpanel MCP (Model Context Protocol)
MIXPANEL_MCP_SERVER_PATH=npx
MIXPANEL_MCP_SERVER_ARGS=-y mcp-remote https://mcp.mixpanel.com/sse --allow-http
MIXPANEL_MCP_ENABLED=true
MIXPANEL_MCP_READ_ONLY=true
MIXPANEL_MCP_MAX_RESULTS=1000
MIXPANEL_MCP_TIMEOUT=300

# Crayon MCP (Model Context Protocol)
CRAYON_MCP_ENABLED=true
CRAYON_MCP_READ_ONLY=true
CRAYON_MCP_MAX_RESULTS=100
CRAYON_MCP_TIMEOUT=300
CRAYON_MCP_AUDIT_LOGGING=true

# Crayon API Credentials
CRAYON_API_KEY=your_crayon_api_key
CRAYON_ACCOUNT_ID=your_crayon_account_id
CRAYON_WORKSPACE_ID=your_crayon_workspace_id

# Crayon MCP Server Configuration
CRAYON_MCP_SERVER_PATH=npx
CRAYON_MCP_SERVER_ARGS=-y @crayon/mcp-server --api-key $CRAYON_API_KEY
CRAYON_MCP_BASE_URL=https://api.crayon.co

# Intercom MCP (Model Context Protocol)
INTERCOM_MCP_ENABLED=true
INTERCOM_MCP_READ_ONLY=true
INTERCOM_MCP_MAX_RESULTS=100
INTERCOM_MCP_TIMEOUT=300
INTERCOM_MCP_AUDIT_LOGGING=true

# Intercom API Credentials
INTERCOM_ACCESS_TOKEN=your_intercom_access_token

# Intercom MCP Server Configuration
INTERCOM_MCP_SERVER_PATH=npx
INTERCOM_MCP_SERVER_ARGS=-y @intercom/mcp-server --access-token $INTERCOM_ACCESS_TOKEN
INTERCOM_MCP_BASE_URL=https://api.intercom.io

# HubSpot MCP (Model Context Protocol)
HUBSPOT_MCP_ENABLED=true
HUBSPOT_MCP_READ_ONLY=true
HUBSPOT_MCP_MAX_RESULTS=100
HUBSPOT_MCP_TIMEOUT=300
HUBSPOT_MCP_AUDIT_LOGGING=true

# HubSpot MCP Server Configuration
HUBSPOT_MCP_SERVER_PATH=npx
HUBSPOT_MCP_SERVER_ARGS=-y @hubspot/mcp-server --client-id $HUBSPOT_CLIENT_ID --client-secret $HUBSPOT_CLIENT_SECRET
HUBSPOT_MCP_BASE_URL=https://api.hubapi.com

# Gong MCP (Model Context Protocol)
GONG_MCP_ENABLED=true
GONG_MCP_READ_ONLY=true
GONG_MCP_MAX_RESULTS=100
GONG_MCP_TIMEOUT=300
GONG_MCP_AUDIT_LOGGING=true

# Gong API Credentials
GONG_API_KEY=your_gong_api_key

# Gong MCP Server Configuration
GONG_MCP_SERVER_PATH=npx
GONG_MCP_SERVER_ARGS=-y @gong/mcp-server --api-key $GONG_API_KEY
GONG_MCP_BASE_URL=https://api.gong.io

# Clay MCP (Model Context Protocol)
CLAY_MCP_ENABLED=true
CLAY_MCP_READ_ONLY=true
CLAY_MCP_MAX_RESULTS=100
CLAY_MCP_TIMEOUT=300
CLAY_MCP_AUDIT_LOGGING=true

# Clay API Credentials
CLAY_API_KEY=your_clay_api_key

# Clay MCP Server Configuration
CLAY_MCP_SERVER_PATH=npx
CLAY_MCP_SERVER_ARGS=-y @clay/mcp-server --api-key $CLAY_API_KEY
CLAY_MCP_BASE_URL=https://api.clay.com

# DataForSEO
DATAFORSEO_LOGIN=your_dataforseo_login
DATAFORSEO_PASSWORD=your_dataforseo_password

# Slack
SLACK_BOT_TOKEN=xoxb-your-slack-bot-token
SLACK_SIGNING_SECRET=your_slack_signing_secret

# Jira
JIRA_BASE_URL=https://your-domain.atlassian.net
JIRA_USER_EMAIL=your_jira_email
JIRA_API_TOKEN=your_jira_api_token

# Webflow
WEBFLOW_CLIENT_ID=your_webflow_client_id
WEBFLOW_CLIENT_SECRET=your_webflow_client_secret
WEBFLOW_SITE_ID=your_webflow_site_id

# Development mode (set to true to use mock data)
NODE_ENV=development
